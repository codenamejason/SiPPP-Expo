{
  "expo": {
    "name": "@dnd",
    "slug": "debate-and-donate",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "debateanddonate",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#cdabf8"
    },
    "ios": {
      "bundleIdentifier": "xyz.debateanddonate.app",
      "supportsTablet": true
    },
    "android": {
      "package": "xyz.debateanddonate.app",
      "adaptiveIcon": {
        "backgroundColor": "#cdabf8"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "privyAppId": "cly0llp2p0l143yxzoqz5ibiy"
    }
  }
}